<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.torque">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Torque"
        android:theme="@style/Theme.TorqueLight">

        <activity
            android:name=".Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MenuPrincipal"
            android:exported="false" />

        <activity android:name=".configuracion.Configuracion" />

        <activity android:name=".mantenimientos.Mantenimiento" />
        <activity android:name=".mantenimientos.MantenimientoPreview" />
        <activity android:name=".mantenimientos.MantenimientoHistorial" />

        <activity android:name=".garaje.MotoDetalle" />
        <activity android:name=".garaje.AgregarMoto" />
        <activity android:name=".garaje.BorrarMoto" />
        <activity android:name=".garaje.ListaGaraje" />
    <activity android:name=".usuarios.PerfilUsuario"/>

    </application>
</manifest>